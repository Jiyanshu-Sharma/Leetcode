class Solution {
    public boolean isThree(int n) {
        int temp = 0;
        int ans = 0;
        for(int i = 1 ; i<= n;i++)
        {
            if(n%i == 0)
            {
                temp++;
            }
           
        }
        if(temp==3)
        {
            return true;
        }
        else 
            return false;
        
    }
}
