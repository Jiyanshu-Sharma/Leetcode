class Solution {
    public int arraySign(int[] nums) {
        int len = nums.length;
        double mul = 1;
        for(int i = 0; i<len;i++)
        {
            mul = mul*nums[i];
        }
        if(mul>0)
        {
            return 1;
        }
        else if(mul<0)
        {
            return -1;
        }
        else
        {
            return 0;
        } 
        
        
    }
}
