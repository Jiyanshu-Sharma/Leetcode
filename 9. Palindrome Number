class Solution {
    public boolean isPalindrome(int x)
    {
        int y = x;
        int res = 0;
        int b = 0;
        while(x!=0)
        {
            b = x%10;
            res = res*10 + b;
            x = x/10;
        }
        if(y == res && y>=0)
            return true;
        else
            return false; 

        
    }
}



       
    
